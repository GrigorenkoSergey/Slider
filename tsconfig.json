{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "module": "ES2015",
    "target": "es5",
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
  },
  "exclude": [
    "node_modules/",
    "dist/",
    "coverage/"
  ],
}