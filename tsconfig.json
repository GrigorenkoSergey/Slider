{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true, //Добавил для корректной работы jquery
    "module": "ES2015",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "sourceMap": true,
  },
  "exclude": [
    "node_modules",
  ],
}